<%= form_for(@entrega) do |f| %>
  <% if @entrega.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@entrega.errors.count, "error") %> prohibited this entrega from being saved:</h2>
      <ul>
      <% @entrega.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
</br>
<% if getAux!=nil && getAux!="" &&  getAux >= 1 %>
 <div class="field">
    <%= f.text_field :preco, :class => 'inputtext2 text_field DOMControl_placeholder', :placeholder => "Preco" %><p>
  </div></br>
<% else %>
  <div class="field">
    <%= f.select :statusentrega, @statusentrega.collect { |c| [ c.status, c.id ] } %>
