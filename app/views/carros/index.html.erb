<h1>Listing carros</h1>

<table>
  <tr>
    <th>Marca</th>
    <th>Modelo</th>
    <th>Ano</th>
    <th>Cor</th>
    <th>Placa</th>
    <th>Iduser</th>
    <th>Idrota</th>
    <th>Photo file name</th>
    <th>Photo content type</th>
    <th>Photo file size</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @carros.each do |carro| %>
  <tr>
    <td><%= carro.marca %></td>
    <td><%= carro.modelo %></td>
    <td><%= carro.ano %></td>
    <td><%= carro.cor %></td>
    <td><%= carro.placa %></td>
    <td><%= carro.idUser %></td>
    <td><%= carro.idRota %></td>
    <td><%= carro.photo_file_name %></td>
    <td><%= carro.photo_content_type %></td>
    <td><%= carro.photo_file_size %></td>
    <td><%= link_to 'Show', carro %></td>
    <td><%= link_to 'Edit', edit_carro_path(carro) %></td>
    <td><%= link_to 'Destroy', carro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Carro', new_carro_path %>
